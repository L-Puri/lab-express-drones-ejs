<h2>Update the drone</h2>

<form action="/drones/<%= drone._id%>/edit" method="post">
    <label> name:
        <input type="String" name="name" value="<%= drone.name %>" />
    </label>
    
    <label> propellers:
        <input type="number" name="propellers" value="<%= drone.propellers %>" />
    </label>
    
    <label> maxSpeed:
        <input type="number" name="maxSpeed" value="<%= drone.maxSpeed %>" />
    </label>
    
    <button type="submit"> Update drone </button>
    </form>
    